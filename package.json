{
  "name": "brello-effector",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:css": "npx stylelint \"**/*.{css}\" --allow-empty-input",
    "lint:css:fix": "npx stylelint \"**/*.{css}\" --fix",
    "lint:fsd": "npx steiger ./src",
    "fsd:watch": "npx steiger ./src -w",
    "fsd:fix": "npx steiger ./src -f",
    "knip": "knip",
    "knip:md": "knip --reporter markdown > knip-report.md",
    "knip:fix": "knip --fix-type exports,types,files --allow-remove-files",
    "reinstall": "rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "@mantine/core": "^8.2.1",
    "@mantine/dates": "^8.2.1",
    "@mantine/dropzone": "^8.2.1",
    "@mantine/hooks": "^8.2.1",
    "@mantine/modals": "^8.2.1",
    "@mantine/notifications": "^8.2.1",
    "@mantine/spotlight": "^8.2.1",
    "@mantine/tiptap": "^8.2.1",
    "@tabler/icons-react": "^3.34.1",
    "@tiptap/extension-link": "^3.0.7",
    "@tiptap/react": "^3.0.7",
    "@tiptap/starter-kit": "^3.0.7",
    "dayjs": "^1.11.13",
    "modern-normalize": "^3.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "lefthook": "^1.12.2",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "solidarity": "^3.0.4",
    "steiger": "^0.5.9",
    "stylelint": "^16.22.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^7.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
